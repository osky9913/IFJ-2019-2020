.IFJcode19
JUMP $$main

#-----------------------------

LABEL !chr
PUSHFRAME
DEFVAR LF@%%return_value
DEFVAR LF@type
TYPE LF@type LF@%param1
JUMPIFNEQ %chr_wrong_type LF@type string@int
INT2CHAR LF@%%return_value LF@%param1
POPFRAME
RETURN
LABEL %chr_wrong_type
EXIT int@4

#-----------------------------

LABEL $$main # main body
CREATEFRAME		#----no main-body vars
PUSHFRAME
DEFVAR LF@y
CREATEFRAME		#----
DEFVAR TF@%param1
CALL !chr
MOVE LF@y TF@%%return_value
WRITE LF@y
# end of main body
